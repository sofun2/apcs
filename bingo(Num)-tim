
/**
 * Write a description of class RandomGen here.
 * 
 * @author (your name) 
 * @version (a version number or a date)
 */
import java.util.*;
public class Num
{
    int num;
    
    public int getNum()
    {
        return num;
    }
    
    public int gen()
    {
        Random rand = new Random();
        num = rand.nextInt(99) + 1;
        return num;
    }
}
